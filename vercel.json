{
  "version": 2,
  "builds": [
    {
      "src": "server-mongodb.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-mongodb.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ],
  "functions": {
    "server-mongodb.js": {
      "maxDuration": 30
    }
  }
}